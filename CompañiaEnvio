package diagrama;

public class CompañiaEnvio {
	double precioEntrega;

	public double elegirCompañia(int x) {
		Pedido p = new Pedido();
		Carrito c = new Carrito();
		double precioEnvio = p.getPrecioFinal(c.carrito);
		if (x == 1){
			  precioEnvio += 5;
		}else if(x == 2){
			 precioEnvio += 3;
		}else if(x == 3){
			 precioEnvio +=4;
		}
		return precioEnvio;
		
	}
}
